{
  "name": "universal-react-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hilzu/universal-react-starter.git"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify src/client/client.js > public/bundle.js",
    "start": "nodemon src/server/server.js",
    "watch": "watchify src/client/client.js -o public/bundle.js -dv"
  },
  "dependencies": {
    "babel-core": "^6.1.4",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-react": "^6.1.4",
    "consolidate": "^0.13.1",
    "express": "^4.13.3",
    "history": "^1.11.1",
    "mustache": "^2.1.3",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-router": "^1.0.0-rc1"
  },
  "license": "Public Domain",
  "devDependencies": {
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "nodemon": "^1.4.1",
    "watchify": "^3.2.3"
  }
}
